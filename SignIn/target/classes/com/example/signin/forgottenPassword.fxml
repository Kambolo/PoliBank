<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="230.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.signin.ForgottenPasswordController">
   <children>
      <Label layoutX="127.0" layoutY="14.0" text="Zapomniałeś hasła?">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="39.0" layoutY="89.0" prefHeight="20.0" prefWidth="68.0" text="E-mail">
         <font>
            <Font name="Lucida Console" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="email" layoutX="39.0" layoutY="115.0" prefHeight="26.0" prefWidth="224.0" />
      <Button layoutX="297.0" layoutY="99.0" mnemonicParsing="false" onAction="#checkEmail" prefHeight="40.0" prefWidth="82.0" text="Wyślij" />
      <Label fx:id="message" layoutX="39.0" layoutY="162.0" text="Label" />
      <Button layoutX="298.0" layoutY="151.0" mnemonicParsing="false" onAction="#switchToSignIn" prefHeight="40.0" prefWidth="82.0" text="Zaloguj" />
      <Label layoutX="39.0" layoutY="53.0" text="Podaj adres email konta do którego hasła zapomniłeś/aś." />
   </children>
</AnchorPane>
